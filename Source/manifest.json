{
	"manifest_version": 3,
	"name": "NFT to silly jpeg",
	"version": "1.0",
	"description": "Replaces the text 'NFT' with 'silly jpg'.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"host_permissions": [
  		"*://*/*"
	],
	"permissions": [
		"activeTab"
	]
}

